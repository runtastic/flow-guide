{
  "parser": "babel-eslint",
  "plugins": ["react", "flowtype"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "Generator": true
  },
  "rules": {
    "new-cap": [2, { "capIsNewExceptions": ["Map", "List"] }],
    "no-use-before-define": 0,
    "max-len": [2, 140, 4],
    "eqeqeq": [2, "smart"]
  }
}
